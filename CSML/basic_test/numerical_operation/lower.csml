////////////////////////////////////////////////////////////////////////////////
/// LOWER ARRAY
////////////////////////////////////////////////////////////////////////////////

start:
    goto end

lower_array_step_0:
    say [] < []

lower_array_step_1:
    say [] < true

lower_array_step_2:
    say [] < 1.0

lower_array_step_3:
    say [] < 1

lower_array_step_4:
    say [] < null

lower_array_step_5:
    say [] < {}

lower_array_step_6:
    say [] < "1"

