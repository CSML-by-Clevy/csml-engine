////////////////////////////////////////////////////////////////////////////////
/// AND ARRAY
////////////////////////////////////////////////////////////////////////////////

start:
    goto end

and_array_step_0:
    say [] & []

and_array_step_1:
    say [] & true

and_array_step_2:
    say [] & 1.0

and_array_step_3:
    say [] & 1

and_array_step_4:
    say [] & null

and_array_step_5:
    say [] & {}

and_array_step_6:
    say [] & "1"

