start:
    use 4 as var1
    use 2 as var2

    say var1 / var2
    goto end

div2:
    use 4 as var1
    use 2 as var2
    say (var1 + var1) / (var2 + var1) + (2 * 10)
    goto end

div3:
    say 4 / 0 + (2 * 10)

    goto end

