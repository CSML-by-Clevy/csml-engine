////////////////////////////////////////////////////////////////////////////////
/// GREATER ARRAY
////////////////////////////////////////////////////////////////////////////////

start:
    goto end

greater_array_step_0:
    say [] > []

greater_array_step_1:
    say [] > true

greater_array_step_2:
    say [] > 1.0

greater_array_step_3:
    say [] > 1

greater_array_step_4:
    say [] > null

greater_array_step_5:
    say [] > {}

greater_array_step_6:
    say [] > "1"

