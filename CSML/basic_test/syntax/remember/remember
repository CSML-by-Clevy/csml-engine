////////////////////////////////////////////////////////////////////////////////
/// REMEMBER VALID SYNTAX
////////////////////////////////////////////////////////////////////////////////

start:
    // pass
    remember key = value

start:
    // pass
    remember key = value as tmp

start:
    // pass
    remember value as key

start:
    // pass
    remember "" as key

////////////////////////////////////////////////////////////////////////////////
/// REMEMBER VALID SYNTAX WITH ERROR OF ASSIGNATION
////////////////////////////////////////////////////////////////////////////////

start:
    // fail
    remember event = value

////////////////////////////////////////////////////////////////////////////////
/// USE INVALID SYNTAX
////////////////////////////////////////////////////////////////////////////////

start:
    // fail
    remember Audio

start:
    // fail
    remember Audio()