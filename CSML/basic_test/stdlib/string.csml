start:
    goto end

step_0:
    remember s = "Hello "

    do s.append("World")

    say s

    do s.to_uppercase()

    say s

    say s.to_lowercase()

step_1:
    remember s = "Hello"

    say s.contains("H")
    say s.contains("Hello")
    say s.contains_regex("(?i)He")

step_2:
    remember s = "Hello"

    say s.starts_with_regex("(?i)He")
    say s.ends_with("o")
    say s.starts_with("el")
    say s.starts_with("O")
    say s.ends_with_regex("[l]")

step_3:
    say "Hello".contains("H")