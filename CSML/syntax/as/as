////////////////////////////////////////////////////////////////////////////////
/// AS VALID SYNTAX
////////////////////////////////////////////////////////////////////////////////

start:
    // pass
    say value as key

////////////////////////////////////////////////////////////////////////////////
/// AS VALID SYNTAX WITH ERROR OF ASSIGNATION
////////////////////////////////////////////////////////////////////////////////

start:
    // fail
    say value as event

////////////////////////////////////////////////////////////////////////////////
/// AS INVALID SYNTAX
////////////////////////////////////////////////////////////////////////////////

start:
    // fail
    say value as obj.key

start:
    // fail
    say value as key as key

start:
    // fail
    say value as key = value

start:
    // fail
    say value as ""

start:
    // fail
    say value as Audio()