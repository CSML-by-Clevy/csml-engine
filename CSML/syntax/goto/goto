////////////////////////////////////////////////////////////////////////////////
/// GOTO VALID SYNTAX
////////////////////////////////////////////////////////////////////////////////

start:
    // pass
    goto key

start:
    // pass
    goto step key

start:
    // pass
    goto flow key

////////////////////////////////////////////////////////////////////////////////
/// GOTO VALID SYNTAX WITH ERROR OF ASSIGNATION
////////////////////////////////////////////////////////////////////////////////

start:
    // fail
    goto event

start:
    // fail
    goto step event

start:
    // fail
    goto flow event

////////////////////////////////////////////////////////////////////////////////
/// GOTO INVALID SYNTAX
////////////////////////////////////////////////////////////////////////////////

start:
    // fail
    goto event as tmp

start:
    // fail
    goto event = value

start:
    // fail
    goto key as tmp

start:
    // fail
    goto key = value

start:
    // fail
    goto lol key

start:
    // fail
    goto step key = value

start:
    // fail
    goto step key as tmp

start:
    // fail
    goto step key flow as tmp

start:
    // fail
    goto obj.key