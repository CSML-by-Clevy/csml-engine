////////////////////////////////////////////////////////////////////////////////
/// DO VALID SYNTAX
////////////////////////////////////////////////////////////////////////////////

start:
    // pass
    do key = value

start:
    // pass
    do key = value as tmp

start:
    // pass
    do value as key

start:
    // pass
    do "" as key

start:
    // pass
    do 42

start:
    // pass
    do Audio()

////////////////////////////////////////////////////////////////////////////////
/// DO VALID SYNTAX WITH ERROR OF ASSIGNATION
////////////////////////////////////////////////////////////////////////////////

start:
    // fail
    do foreach as key

////////////////////////////////////////////////////////////////////////////////
/// DO INVALID SYNTAX
////////////////////////////////////////////////////////////////////////////////

start:
    // fail
    do value value as key

start:
    // fail
    do value as key key