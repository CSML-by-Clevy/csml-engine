///////////////////////////////////////////////////////////////////////////////
/// REMEMBER
////////////////////////////////////////////////////////////////////////////////

remember key = value
remember key = value as tmp
remember value as key

////////////////////////////////////////////////////////////////////////////////
/// USE
////////////////////////////////////////////////////////////////////////////////

use value as key
use obj.value as key

////////////////////////////////////////////////////////////////////////////////
/// DO
////////////////////////////////////////////////////////////////////////////////

do key = value
do key = value as tmp

do value as key
do obj.value as key

do 42
do Audio()

////////////////////////////////////////////////////////////////////////////////
/// GOTO
////////////////////////////////////////////////////////////////////////////////

goto key
goto step key
goto flow key

////////////////////////////////////////////////////////////////////////////////
/// AS
////////////////////////////////////////////////////////////////////////////////

... as key

////////////////////////////////////////////////////////////////////////////////
/// FOREACH
////////////////////////////////////////////////////////////////////////////////

foreach(key) in key
foreach(key) in key as tmp
foreach(key, value) in key
foreach(key, value) in key as tmp

////////////////////////////////////////////////////////////////////////////////
/// IF
////////////////////////////////////////////////////////////////////////////////

if (key)
if (key as tmp)