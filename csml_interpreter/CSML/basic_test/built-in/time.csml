start:
    do time = Time()

    say time.milliseconds.is_int()
    say time.unix().is_int()

    do time.at(2014, 10, 20)
    say time.format()

    say time.format("%Y")
    goto end

parse_1_arg:
    do time = Time().parse("1983-08-13")

    say time.format()
    goto end

parse_2_arg:
    do time = Time().parse("1983 08 13 12:09:14.274", "%Y %m %d %H:%M:%S%.3f")

    say time.format()
    goto end

parse_timezone:
    do time = Time().parse("2014-11-28T21:00:09+09:00")

    say time.format()
    goto end

with_timezone:
    do time = Time().parse("2014-11-28T21:00:09Z").with_timezone(9 * 3600)

    say time.format()
    goto end