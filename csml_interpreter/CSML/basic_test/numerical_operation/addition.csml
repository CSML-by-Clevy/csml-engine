addition_array_step_0:
    say [] + []
    goto end

addition_array_step_1:
    say [] + true
    goto end

addition_array_step_2:
    say [] + 1.0
    goto end

addition_array_step_3:
    say [] + 1
    goto end

addition_array_step_4:
    say [] + null
    goto end

addition_array_step_5:
    say [] + {}
    goto end

addition_array_step_6:
    say [] + "1"
    goto end

