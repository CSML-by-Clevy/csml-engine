////////////////////////////////////////////////////////////////////////////////
/// LOWER ARRAY
////////////////////////////////////////////////////////////////////////////////

start:
    goto end

lower_array_step_0:
    say [] < []
    goto end

lower_array_step_1:
    say [] < true
    goto end

lower_array_step_2:
    say [] < 1.0
    goto end

lower_array_step_3:
    say [] < 1
    goto end

lower_array_step_4:
    say [] < null
    goto end

lower_array_step_5:
    say [] < {}
    goto end

lower_array_step_6:
    say [] < "1"
    goto end

