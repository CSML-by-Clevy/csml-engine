start:
    use 4 as var1
    use 2 as var2

    say var1 / var2
    goto end

div2:
    use 4 as var1
    use 2 as var2
    say (var1 + var1) / (var2 + var1) + (2 * 10)
    goto end

div3:
    say 4 / 0 + (2 * 10)
    goto end

////////////////////////////////////////////////////////////////////////////////
/// DIVISION ARRAY
////////////////////////////////////////////////////////////////////////////////

division_array_step_0:
    say [] / []
    goto end

division_array_step_1:
    say [] / true
    goto end

division_array_step_2:
    say [] / 1.0
    goto end

division_array_step_3:
    say [] / 1
    goto end

division_array_step_4:
    say [] / null
    goto end

division_array_step_5:
    say [] / {}
    goto end

division_array_step_6:
    say [] / "1"
    goto end

